<OptGrid@GridLayout>:
    cols: 2
    row_force_default: True
    row_default_height: 50
    pos_hint: {'center_x':.5}
    size_hint: (None, None)
    size: (400, self.height)
    # size is updated whenever minimum_size is.
    spacing: 20
    # top is updated whenever height is.
    top: self.height
<Label>:
    spacing:(50,50)
    font_size: 26
<ToggleButton>:
    allow_no_selection: False
    size_hint_x: 0.2
    font_size: 18


<Scroller>:
